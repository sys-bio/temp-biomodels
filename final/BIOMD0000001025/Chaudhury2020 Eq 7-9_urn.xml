        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <divide />
            <cn>1</cn>
            <apply>
              <plus />
              <cn>1</cn>
              <apply>
                <power />
                <apply>
                  <divide />
                  <ci>U_crit</ci>
