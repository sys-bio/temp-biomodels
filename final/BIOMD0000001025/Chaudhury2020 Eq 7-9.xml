      <assignmentRule variable="CAR_T_cells">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <plus/>
            <ci> Expanding_CAR_T_cells </ci>
            <ci> Persistent_CAR_T_cells </ci>
          </apply>
        </math>
            </assignmentRule>
      <assignmentRule variable="F_cure">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <divide/>
            <cn> 1 </cn>
            <apply>
              <plus/>
              <cn> 1 </cn>
              <apply>
                <power/>
                <apply>
                  <divide/>
                  <ci> U_crit </ci>
                  <ci> Tumor_cells </ci>
                </apply>
                <ci> h </ci>
              </apply>
            </apply>
          </apply>
        </math>
            </assignmentRule>
      <rateRule variable="Expanding_CAR_T_cells">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <piecewise>
            <piece>
              <apply>
                <times/>
                <ci> rho </ci>
                <apply>
                  <divide/>
                  <apply>
                    <times/>
                    <ci> Expanding_CAR_T_cells </ci>
                    <ci> Tumor_cells </ci>
                  </apply>
                  <apply>
                    <plus/>
                    <ci> EC_50_rho </ci>
                    <ci> Expanding_CAR_T_cells </ci>
                    <ci> Tumor_cells </ci>
                  </apply>
                </apply>
              </apply>
              <apply>
                <leq/>
                <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> time </csymbol>
                <ci> T_max </ci>
              </apply>
            </piece>
            <otherwise>
              <apply>
                <times/>
                <apply>
                  <minus/>
                  <apply>
                    <plus/>
                    <ci> k_EP </ci>
                    <ci> k_E </ci>
                  </apply>
                </apply>
                <ci> Expanding_CAR_T_cells </ci>
              </apply>
            </otherwise>
          </piecewise>
        </math>
            </rateRule>
      <rateRule variable="Persistent_CAR_T_cells">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <piecewise>
            <piece>
              <cn> 0 </cn>
              <apply>
                <leq/>
                <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> time </csymbol>
                <ci> T_max </ci>
              </apply>
            </piece>
            <otherwise>
              <apply>
                <minus/>
                <apply>
                  <times/>
                  <ci> k_EP </ci>
                  <ci> Expanding_CAR_T_cells </ci>
                </apply>
                <apply>
                  <times/>
                  <ci> beta </ci>
                  <ci> Persistent_CAR_T_cells </ci>
                </apply>
              </apply>
            </otherwise>
          </piecewise>
        </math>
            </rateRule>
    </listOfRules>
