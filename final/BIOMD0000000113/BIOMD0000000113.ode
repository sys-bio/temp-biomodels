par n=2.0

# Parameter:   id =  Kp, name = Kp, constant
par Kp=1.0

# Parameter:   id =  Vm3, name = Vm3, constant
par Vm3=500.0

# Parameter:   id =  m, name = m, constant
par m=2.0

# Parameter:   id =  Kr, name = Kr, constant
par Kr=2.0

# Parameter:   id =  K_A, name = K_A, constant
par K_A=0.9

# Parameter:   id =  kf, name = kf, constant
par kf=1.0

# Parameter:   id =  k, name = k, constant
par k=10.0

# Parameter:   id =  p, name = p, constant
