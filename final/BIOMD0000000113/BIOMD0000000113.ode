par vMK=20.0

# Parameter:   id =  vp, name = vp, constant
par vp=2.5

# Parameter:   id =  Ka, name = Ka, constant
par Ka=2.5

# Parameter:   id =  q, name = q, constant
par q=1.0

# Parameter:   id =  K1, name = K1, constant
par K1=0.01

# Parameter:   id =  K2, name = K2, constant
par K2=0.01

# Parameter:   id =  v1_beta, name = v1_beta, constant
par v1_beta=2.7

# assignmentRule: variable = vk
vk=vMK*Z^q/(Ka^q+Z^q)
aux vk=vk

# Reaction: id = R0, name = Calcium influx

R0=cytosol*v0

# Reaction: id = R1, name = InsP3 dependent Ca influx

R1=cytosol*v1_beta

# Reaction: id = R2, name = ATP driven pumping of Ca into store

R2=cytosol*Vm2*Z^n/(Kp^n+Z^n)

# Reaction: id = R3, name = ATP driven pumping into cytosol

R3=store*Vm3*Y^m*Z^p/((Kr^m+Y^m)*(K_A^p+Z^p))

# Reaction: id = Rf, name = Calcium leak

Rf=store*kf*Y

# Reaction: id = R_eff, name = Ca efflux

R_eff=cytosol*k*Z

# Reaction: id = Protein_Phosphorylation, name = Protein Phosphorylation

Protein_P=cytosol*vp/Wt*(vk/vp*(1-W_star)/(K1+1-W_star)-W_star/(K2+W_star))

# Species:   id = Z, name = Z, affected by kineticLaw

init Z=0.15
dZ/dt=(1/(cytosol))*(( 1.0 * R0) + ( 1.0 * R1) + (-1.0 * R2) + ( 1.0 * R3) + ( 1.0 * Rf) + (-1.0 * R_eff))

# Species:   id = Y, name = Y, affected by kineticLaw

init Y=1.6
dY/dt=(1/(store))*(( 1.0 * R2) + (-1.0 * R3) + (-1.0 * Rf))

# Species:   id = Wt, name = Total Protein
# Warning species is not changed by either rules or reactions

par Wt=1.0
aux Wt=Wt

# Species:   id = W_star, name = Phosphorylated protein, affected by kineticLaw

