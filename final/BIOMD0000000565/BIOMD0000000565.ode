# Local Parameter:   id =  kcat_CURS, name = kcat_CURS
par kcat_CURS=0.02163

# Local Parameter:   id =  Km_CURS_fercoa, name = Km_CURS_fercoa
par Km_CURS_f=0.018

# Local Parameter:   id =  Km_CURS_ferdicoa, name = Km_CURS_ferdicoa
par Km_CURS_1=0.018


CURS=E_CURS*kcat_CURS*fercoa/(Km_CURS_f+fercoa)*ferdicoa/(Km_CURS_1+ferdicoa)

# Reaction: id = FER_t
# Local Parameter:   id =  k_FER_t, name = k_FER_t
